.container {
  width: 750rpx;
  min-height: 100vh;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  background-color: #f9f9f9;
  background-image: linear-gradient(180deg, rgba(255,106,76,0.12) 0%, rgba(255,107,77,0.00) 79%);
}
.container{
  --tabs-nav-background-color: transparent;
}
.top-tab {
  margin-top: 24rpx;
  width: 750rpx;
  color: #000000;
}
.mx-tabs__nav{
  width: 352rpx;
  margin-left: 198rpx;
}
.tab-page {
  width: 750rpx;
  display: flex;
  position: relative;
  flex-direction: column;
}
.bottom-block{
  height: 208rpx;
  width: 750rpx;
  background-color:#FFFFFF;
  display: flex;
  flex-direction: row;
  justify-content: space-evenly;
  align-items: flex-start;
  position: fixed;
  left: 0;
  bottom: 0;
  padding-top: 18rpx;
}
.btn-text{
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  font-weight: 500;
}
.timing-text {
  width: 750rpx;
  font-family: MEIDITYPE-MEDIUM;
  font-size: 96rpx;
  line-height: 120rpx;
  color: #000000;
  text-align: center;
  font-weight: 500;
  margin-top: 32rpx;
}
.timing-start-text {
  width: 750rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  color: #666666;
  text-align: center;
  font-weight: 400;
  margin-top: 8rpx;
}
.time-block {
  width: 750rpx;
  height: 440rpx;
  display: flex;
  position: relative;
}
.btn-left {
  width: 192rpx;
  height: 192rpx;
  border-radius: 96rpx;
  position: absolute;
  left: 96rpx;
  top: 82rpx;
  box-shadow: 0px 1px 10px 0px rgba(0,0,0,0.04);
}
.btn-right {
  width: 192rpx;
  height: 192rpx;
  border-radius: 96rpx;
  position: absolute;
  right: 96rpx;
  top: 82rpx;
  box-shadow: 0px 1px 10px 0px rgba(0,0,0,0.04);
}
.btn-image {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  left: 56rpx;
  top: 56rpx;
}
.long-tip {
  width: 192rpx;
  font-family: PingFangSC-Regular;
  font-size: 22rpx;
  color: #666666;
  text-align: center;
  font-weight: 400;
  position: absolute;
  left: 0;
  bottom: 18rpx;
}
.lr-time-text {
  width: 375rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #333333;
  text-align: center;
  font-weight: 400;
  position: absolute;
  top: 314rpx;
}
.note-block{
  width: 686rpx;
  border-radius: 32rpx;
  background-color: #FFFFFF;
  margin-top: 24rpx;
  margin-left: 32rpx;
  display: flex;
  flex-direction: column;
  box-shadow: 0px 1px 10px 0px rgba(0,0,0,0.04);
}
.note-title{
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #333333;
  font-weight: 500;
  margin-top: 28rpx;
  margin-left: 44rpx;
}
.input-block{
  width: 602rpx;
  min-height: 96rpx;
  margin-top: 28rpx;
  margin-left: 40rpx;
  margin-bottom: 52rpx;
}
.mon-type-data-block{
  width: 686rpx;
  display: flex;
  flex-direction: column;
  border-radius: 32rpx;
  background-color: #FFFFFF;
  margin-top: 24rpx;
  margin-left: 32rpx;
  min-height: 104rpx;
  overflow: hidden;
}
.cell-arrow{
  margin-left: 16rpx;
}

